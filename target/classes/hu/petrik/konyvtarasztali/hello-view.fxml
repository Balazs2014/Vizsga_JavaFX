<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="339.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.petrik.konyvtarasztali.HelloController">
   <children>
      <VBox prefHeight="339.0" prefWidth="530.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onTorlesClick" prefHeight="25.0" prefWidth="59.0" text="Törlés" />
            <TableView fx:id="konyvTableView" prefHeight="288.0" prefWidth="510.0">
              <columns>
                <TableColumn fx:id="cimCol" prefWidth="75.0" text="Cím" />
                <TableColumn fx:id="szerzoCol" prefWidth="75.0" text="Szerző" />
                  <TableColumn fx:id="kiadasEveCol" prefWidth="75.0" text="Kiadás éve" />
                  <TableColumn fx:id="oldalszamCol" prefWidth="75.0" text="Oldalszám" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
